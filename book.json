{
     "title": "编写gitbook电子书教程",
    "description": "gitbook电子书教程",
    "author": "darrenliuwei",
    "language": "zh-hans",
    "root": ".",
    "plugins": [
    "toggle-chapters",
    "splitter",
    "-lunr",
    "-search",
    "search-plus",
     "my-toolbar",
     "-sharing",
     "back-to-top-button",
     "anchors"
    ],
    "pluginsConfig": {
        "tags": {
            "placement": "bottom"
        },
        "etoc": {
            "h2lb": 3,
            "mindepth": 3,
            "maxdepth": 4,
            "notoc": false
        },
        "my-toolbar": {
            "buttons":
            [
                {
                    "label": "Share page title and link on Twitter",
                    "icon": "fa fa-twitter",
                    "position":"left",
                    "url": "http://baidu.com"
                } 
            ]
        }
    }
}
