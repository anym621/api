{
    "plugins": [
    "toggle-chapters",
    "splitter",
    "-lunr",
    "-search",
    "search-plus",
     "my-toolbar",
     "-sharing",
     "etoc",
     "tags",
     "back-to-top-button",
     "emphasize" 
    ],
    "pluginsConfig": {
        "tags": {
            "placement": "bottom"
        },
        "etoc": {
            "h2lb": 3,
            "mindepth": 3,
            "maxdepth": 4,
            "notoc": false
        },
        "my-toolbar": {
            "buttons":
            [
                {
                    "label": "Share page title and link on Twitter",
                    "icon": "fa fa-twitter",
                    "position":"left",
                    "url": "http://baidu.com"
                },
                {
                    "label": "Edit page on github",
                    "text":"Edit",
                    "icon": "fa fa-pencil-square-o",
                    "position" : "left",
                    "url": "https://github.com/org/repo/edit/master/{{filepath_lang}}"
                }
            ]
        }
    }
}
